{
  "name": "BL",
  "version": "1.0.0",
  "description": "Vue SPA",
  "author": "valentyn.dorosh",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"nuxt\" \"gulp watch\"",
    "build": "gulp && nuxt build --no-lock",
    "start": "nuxt start",
    "generate": "gulp && nuxt generate --no-lock",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "fast_deploy": "npm run build; git add .; git commit -m 'update'; git push;",
    "build:deploy": "cross-env DEPLOY_ENV=deploy nuxt build",
    "generate:deploy": "cross-env DEPLOY_ENV=deploy nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/google-analytics": "^2.2.0",
    "animejs": "^3.0.1",
    "cross-env": "^5.2.0",
    "dateformat": "^3.0.3",
    "nuxt": "^2.4.0",
    "nuxt-facebook-pixel-module": "^1.3.0",
    "v-money": "^0.8.1",
    "vue-the-mask": "^0.11.1",
    "vuejs-datepicker": "^1.5.4",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@directus/sdk-js": "^5.2.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "gsap": "^3.6.0",
    "gulp": "^4.0.0",
    "gulp-iconfont": "^10.0.2",
    "gulp-iconfont-css": "^3.0.0",
    "jsonp": "^0.2.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-owl-carousel": "^2.0.3",
    "vue-slide-bar": "^1.1.9",
    "vue-text-dot": "^1.0.0"
  }
}
